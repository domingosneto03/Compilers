import ioPlus;

class NestedIfWhile {
    
    public int testNestedControl(int a) {
        int i;
        int result;
        
        result = 0;
        i = 0;
        
        while (i < a) {
            if (i < 2) {
                result = result + 1;
            } else {
                result = result + 2;
            }
            
            i = i + 1;
        }
        
        return result;
    }
}
